@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
	<thead></thead>
	<tr>
		<th>
			Category Name 
		</th>
		<th>
			Display Order 
		</th>
	</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model.OrderBy(c => c.DisplayOrder))
		{
			<tr>
				<td>@obj.Name</td>
				<td>
					@obj.DisplayOrder
				</td>
			</tr>
		}
	</tbody>
</table>